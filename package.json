{
  "name": "go_dog_api",
  "version": "1.0.0",
  "description": "A simple dog's race api with provides CRUD like operations that enables the user to register a dog/dog's race, list a specific registered dog, list all registered dogs and list all registered dogs with a specific race",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run prebuild && npm run build && npm run migration-run && npm run server",
    "prebuild": "npm i",
    "build": "npx tsc",
    "migration-create": "npx ts-node-dev ./node_modules/typeorm/cli.js migration:create --name",
    "migration-revert": "npx ts-node-dev ./node_modules/typeorm/cli.js migration:revert",
    "migration-run": "npx ts-node-dev ./node_modules/typeorm/cli.js migration:run",
    "server": "node ./dist/server",
    "dev": "ts-node-dev  --inspect --transpile-only --ignore-watch node_modules ./src/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anderson2dev/go_dog_api.git"
  },
  "keywords": [
    "Typescript",
    "dog's",
    "race",
    "api",
    "cutie puppies",
    "pets",
    "express",
    "javascript",
    "nodejs"
  ],
  "author": "Anderson Loiola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/anderson2dev/go_dog_api/issues"
  },
  "homepage": "https://github.com/anderson2dev/go_dog_api#readme",
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-server": "^0.12.3",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.30",
    "uuidv4": "^6.2.6"
  }
}
